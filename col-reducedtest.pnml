											</subterm>
										</lessthanorequal>
									</subterm>
									<subterm>
										<lessthan>
											<subterm>
												<variable refvariable="vards"/>
											</subterm>
											<subterm>
												<variable refvariable="varda"/>
											</subterm>
										</lessthan>
									</subterm>
								</and>
							</subterm>
							<subterm>
