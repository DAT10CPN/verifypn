													</subterm>
													<subterm>
														<variable refvariable="varda2"/>
													</subterm>
												</lessthan>
											</subterm>
											<subterm>
												<equality>
													<subterm>
														<variable refvariable="vartsp"/>
													</subterm>
													<subterm>
														<useroperator declaration="speed4"/>
													</subterm>
												</equality>
											</subterm>
										</and>
									</subterm>
									<subterm>
										<and>
