				<graphics>
					<position x="0" y="0"/>
				</graphics>
				<condition>
					<text>((varda2 lte vards2 and vards lt varda) or ((vards2 lt varda2 and vartsp eq speed4) or (vartsp eq speed1 and distance1 lt vardb)))</text>
